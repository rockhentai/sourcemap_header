{
  "name": "source_map_header",
  "displayName": "SourceMap Header",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "lilith shenke@lilith.com",
  "scripts": {
    "dev": "plasmo dev --target=2",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "plasmo": "0.85.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "<all_urls>"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "libs/monaco/**/*"
        ]
      }
    ]
  }
}
